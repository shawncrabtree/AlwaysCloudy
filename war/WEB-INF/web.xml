<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>AlwaysSunnyCloudServlet</servlet-name>
		<servlet-class>com.example.alwayssunny.AlwaysSunnyCloudServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AlwaysSunnyCloudServlet</servlet-name>
		<url-pattern>/alwayssunny</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SunnyDataServlet</servlet-name>
		<servlet-class>com.example.alwayssunny.SunnyDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SunnyDataServlet</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
</web-app>
